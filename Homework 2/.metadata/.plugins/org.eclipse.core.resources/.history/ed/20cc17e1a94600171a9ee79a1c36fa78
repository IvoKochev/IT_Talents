import java.util.Scanner;

public class Task12 {

	public static void main(String[] args) {
		int sum, x1 =0, x2=0, x3=0;
		Scanner sc= new Scanner(System.in);
		System.out.println("Enter sum: ");
		sum = sc.nextInt();
		for(int i = 100; i< 1000; i++){
			x1 = i/100;
			x2 = i%10;
			x2 = x2/10;
			x3 = i%10;
			if((x1+x2+x3) == sum){
				System.out.print(i +",");
			}
			
			
		}

	}

}
