import java.util.Scanner;

public class Task7 {

	public static void main(String[] args) {
		int number;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter your number: ");
		number = sc.nextInt();
		for(int i = 3; i<=(number*3); i ++){
			if(i%3 != 0){
				continue;
			}
			System.out.print(i + ",");
		}
		sc.close();

	}

}
