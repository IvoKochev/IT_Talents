import java.util.Scanner;

public class Task21 {

	public static void main(String[] args) {
		int number;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter number [1-52]");
		number = sc.nextInt();
		if(number>0 && number<53){
			for(int i = 0; i< 12; i++){
				for(int j =0; j<4; j++)
					System.out.println(i + " + " +j);
			}
		}

	}

}
